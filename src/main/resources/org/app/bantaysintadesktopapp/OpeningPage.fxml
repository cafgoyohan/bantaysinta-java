<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<?import java.net.URL?>
<StackPane xmlns:fx="http://javafx.com/fxml" fx:controller="org.app.bantaysintadesktopapp.OpeningController"
           fx:id="rootPane" styleClass="stack-pane" stylesheets="@opening.css">
    <stylesheets>
        <URL value="@opening.css"/>
    </stylesheets>
    <VBox alignment="CENTER" spacing="20" styleClass="button-container">
        <Button text="SIGN IN" onAction="#openSignInPage" styleClass="signin-button"/>
    </VBox>
</StackPane>

